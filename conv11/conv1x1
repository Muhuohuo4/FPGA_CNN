module conv_1x1(
    input wire clk,
    input wire rst,
    input wire [15:0] data_in,
    output wire [7:0] data_out
);
    // Internal signals
    wire [7:0] conv_out;

    // Convolution operation
    assign conv_out = data_in * 8'hFF; // Example operation

    // Output assignment
    assign data_out = (rst) ? 8'h00 : conv_out;

endmodule
